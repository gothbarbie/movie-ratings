{
  "name": "movie-ratings",
  "version": "3.1.0",
  "engines": {
    "node": "11.3.0",
    "npm": "6.8.0"
  },
  "private": true,
  "dependencies": {
    "bcrypt": "4.0.1",
    "body-parser": "1.19.0",
    "concurrently": "5.1.0",
    "express": "4.17.1",
    "express-jwt": "5.3.1",
    "express-validator": "5.3.1",
    "jsonwebtoken": "8.5.1",
    "lint-staged": "10.0.8",
    "mongoose": "5.9.5",
    "morgan": "1.10.0",
    "nodemon": "2.0.2",
    "request": "2.88.2",
    "request-promise-native": "1.0.8"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "NODE_ENV=test mocha - timeout 10000",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
